<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Identificacion"
    android:background="@color/gradient_azul">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@color/blanco"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingEnd="26dip"
                android:paddingStart="30dip"
                android:visibility="visible">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/txt_recibe2"/>

                <ImageButton
                    android:id="@+id/ibtAddFront"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginVertical="70dp"
                    android:background="@drawable/ic_action_agregar"
                    android:onClick="clickear"/>

                <ImageView
                    android:id="@+id/imvIneFront"
                    android:layout_width="180dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="50dp"
                    android:layout_marginLeft="20dp"
                    android:src="@android:color/transparent"/>


                <ImageButton
                    android:id="@+id/ibtAddBack"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginVertical="190dp"
                    android:background="@drawable/ic_action_agregar"
                    android:onClick="clickear"/>

                <ImageView
                    android:id="@+id/imvIneBack"
                    android:layout_width="180dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="170dp"
                    android:layout_marginLeft="20dp"
                    android:src="@android:color/transparent"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="290dp"
                    android:text="@string/txt_presta2"/>

                <ImageButton
                    android:id="@+id/ibtAddFront2"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginVertical="370dp"
                    android:background="@drawable/ic_action_agregar"
                    android:onClick="clickear"/>

                <ImageView
                    android:id="@+id/imvIneBack2"
                    android:layout_width="180dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="350dp"
                    android:layout_marginLeft="20dp"
                    android:src="@android:color/transparent"/>

                <ImageButton
                    android:id="@+id/ibtAddBack2"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginVertical="490dp"
                    android:background="@drawable/ic_action_agregar"
                    android:onClick="clickear"/>

                <ImageView
                    android:id="@+id/imvIneFront2"
                    android:layout_width="180dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="470dp"
                    android:layout_marginLeft="20dp"
                    android:src="@android:color/transparent"/>



                <Button
                    android:id="@+id/btnContrato"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="590dp"
                    android:text="@string/btn_contrato"
                    />


            </RelativeLayout>

    </ScrollView>


</LinearLayout>